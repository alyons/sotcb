    "gray-matter": "^4.0.3",
    "next": "^10.2.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.1",
    "remark": "^13.0.0",
    "remark-gfm": "^1.0.0",
